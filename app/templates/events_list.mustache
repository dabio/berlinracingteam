<div class="c1-12">
  <h1 class="page-title">Rennkalender {{ active_year }}</h1>
</div>

<div class="c1-12 entry-content events">

  {{# events }}
  {{# new_month? }}
    <h2>{{ month }}</h2>
  {{/ new_month? }}
  <div class="hentry{{ first }}{{ last }}">
    <h3>
        {{#permalink }}<a href="{{ permalink }}" title=" Rennbericht: {{ title }}">{{ title }} »</a>{{/permalink }}
        {{^permalink }}{{ title }}{{/permalink }}
    </h3>
    <div class="meta">
      <span class="date">{{ date }}</span>
      {{# distance }}<span class="distance"> · {{ distance }} km</span>{{/ distance }}
    </div>
  </div>
  {{/ events }}

</div>

